  <body>
    <%- style %>
    <% if(!((title=="Login" ) || (title=="error" )) ) {%>
      <%if((locals.user)){%>
      <% if(locals.user.position==="student" ){%> 
        <%- include('navbar/student'); %>
      <% } %>
      <% if(locals.user.position==="teacher" ){%>
        <%- include('navbar/teacher'); %>
      <% } %>
      <% if(locals.user.position==="admin" ){%> 
        <%- include('navbar/admin'); %>
