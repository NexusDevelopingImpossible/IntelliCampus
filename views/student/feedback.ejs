<link rel="stylesheet" href="<%= assetPath('css/std-feed.css') %>" />

<div class="main-page">
  <div class="blur">
  <div class="page-heading">
    <!-- <a href="/student/dashboard"><img src="/images/allotarrow.png" alt="" id="back_buttn"></a> -->
    <a href="/student/dashboard"><svg xmlns="http://www.w3.org/2000/svg"  viewBox="0 -960 960 960"><path d="M381-100 44-436q-9-9-13.5-20.5T26-480q0-12 4.5-23.5T44-524l337-337q21-21 51-21.5t52 21.5q21 21 21 51.5T484-758L206-480l278 278q21 21 21.5 50.5T484-100q-21 21-51.5 21T381-100Z"/></svg></a>
    <h1 class="heading">Feedback</h1>
  </div>

  <section class="feedback">
    <%let c=teacherdata.length%>
      <%let g=0%>
        <%for(let i=0; i<teacherdata.length/2; i++){%>
          <div class="feed">
            <%for(let j=0; (j<2 && g<c); j++){%>
           
                <div class="feed-card">
                  <div class="tch-pic">
                    <img src="/images/AG.jpg" alt="" />
                  </div>
                  <div>
                    <div class="tch-name">
                      <%=teacherdata[g].teacherid.name%>
                    </div>
                    <div class="tch-dtl">
                      <%=teacherdata[g].teacherid.position%>
                    </div>
                    <div class="tch-dtl">
                      <%=teacherdata[g].subjectcode.name%>
                    </div>
                  </div>
                  <div class="feed-btn">
                    <a href="/student/response/<%=teacherdata[g].teacherid._id%>">
                    <button class="btn btn-full btn-large">Give Feedback</button>
                  </a>
                  </div>
                </div>
              <%g++%>
           <%}%>
          </div>
        <%}%>
  </section>
  </div>
</div>