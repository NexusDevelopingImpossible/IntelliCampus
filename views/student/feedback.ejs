<div class="main-page">
  <div class="blur">
    <!-- - - - - - -  - -->
    <!-- mobile header -->
    <!-- - - - - - -  - -->
    <div class="mobile-header">
      <a class="mobile-icon" href="/student/dashboard"><img class="logo-phone" src="/images/MANAGER LOGO 3 white 3.png" alt="" /></a>
    <div class="mobile-menu">
      <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 24 24"  fill="#FFFFFF"><path d="M0 0h24v24H0V0z" fill="none"/><path d="M12 22c1.1 0 2-.9 2-2h-4c0 1.1.89 2 2 2zm6-6v-5c0-3.07-1.64-5.64-4.5-6.32V4c0-.83-.67-1.5-1.5-1.5s-1.5.67-1.5 1.5v.68C7.63 5.36 6 7.92 6 11v5l-1.29 1.29c-.63.63-.19 1.71.7 1.71h13.17c.89 0 1.34-1.08.71-1.71L18 16z"/></svg>
      <span id="openBar" onclick="openSidebar()"><svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 24 24" width="5vw" fill="#FFFFFF"><path d="M0 0h24v24H0V0z" fill="none"/><path d="M4 18h16c.55 0 1-.45 1-1s-.45-1-1-1H4c-.55 0-1 .45-1 1s.45 1 1 1zm0-5h16c.55 0 1-.45 1-1s-.45-1-1-1H4c-.55 0-1 .45-1 1s.45 1 1 1zM3 7c0 .55.45 1 1 1h16c.55 0 1-.45 1-1s-.45-1-1-1H4c-.55 0-1 .45-1 1z"/></svg></span>
    </div>
  </div>
  <div class="page-heading">
    <!-- <a href="/student/dashboard"><img src="/images/allotarrow.png" alt="" id="back_buttn"></a> -->
    <a class="back-icon" href="/student/dashboard"><svg xmlns="http://www.w3.org/2000/svg"  viewBox="0 -960 960 960"><path d="M381-100 44-436q-9-9-13.5-20.5T26-480q0-12 4.5-23.5T44-524l337-337q21-21 51-21.5t52 21.5q21 21 21 51.5T484-758L206-480l278 278q21 21 21.5 50.5T484-100q-21 21-51.5 21T381-100Z"/></svg></a>
    <svg class="feedback-icon" xmlns="http://www.w3.org/2000/svg"  viewBox="0 -960 960 960" ><path d="M240-240 106-106q-7 7-16.5 3.5T80-116v-704q0-24 18-42t42-18h680q24 0 42 18t18 42v520q0 24-18 42t-42 18H240Zm240-221 87 53q8 5 16-1t6-16l-23-98 77-67q8-7 4.5-16T634-616l-101-8-39-93q-4-9-14-9t-14 9l-39 93-101 8q-10 1-13.5 10t4.5 16l77 67-23 98q-2 10 6 16t16 1l87-53Z"/></svg>
    <h1 class="heading">Feedback</h1>
  </div>

  <section class="feedback">
    <%let c=teacherdata.length%>
      <%let g=0%>
        <%for(let i=0; i<teacherdata.length/2; i++){%>
          <div class="feed">
            <%for(let j=0; (j<2 && g<c); j++){%>
           
                <div class="feed-card">
                  <div class="tch-pic">
                    <img src="/images/AG.jpg" alt="" />
                  </div>
                  <div>
                    <div class="tch-name">
                      <%=teacherdata[g].teacherid.name%>
                    </div>
                    <div class="tch-dtl">
                      <%=teacherdata[g].teacherid.position%>
                    </div>
                    <div class="tch-dtl">
                      <%=teacherdata[g].subjectcode.name%>
                    </div>
                  </div>
                  <div class="feed-btn">
                    <a href="/student/response/<%=teacherdata[g]._id%>">
                    <button class="btn btn-full btn-large">Give Feedback</button>
                  </a>
                  </div>
                </div>
              <%g++%>
           <%}%>
          </div>
        <%}%>
  </section>
  </div>
    <!-- - - - - - -  - -->
  <!-- mobile side bar2 -->
    <!-- - - - - - -  - -->
  </div>
  <%- include('mobilesidebar'); %>

