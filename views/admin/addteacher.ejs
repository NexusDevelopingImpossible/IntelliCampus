
  <title><%= title%></title>
  <link rel="stylesheet" href="/css/addteacher.css">
  <link rel="stylesheet" href="/css/navbar.css">
  <link
    href="https://fonts.googleapis.com/css2?family=Montserrat:ital,wght@0,100;0,200;0,300;0,400;0,500;0,600;0,700;0,800;0,900;1,100;1,200;1,300;1,400&display=swap"
    rel="stylesheet" />
</head>

<body>

    <div class="main-page">
      <div class="input-page" id="input-pageid">
        <div class="input-heading-1">Add Teachers</div>
        <div class="input-div">
          <div class="form-set">
            <div class="form-text">Registration start</div>
            <input type="text" class="input-name reg-input" name="start" required id="start">
          </div>
          <div class="form-set">
            <div class="form-text">Registration end</div>
            <input type="text" class="input-name reg-input" name="end" required id="end">
          </div>
        </div>
        <button class="input-submit" onclick="create_input_teacher()">Submit</button>
        <p>OR</p>
        <button class=" input-submit import" onclick="create_input_teacher()">Import</button>

      </div>
      <div class="input-page2" id="input-pageid2">
        <div class="input-heading-3">Add Teacher</div>
        <form action="/admin/createteacher" method="post">
          <div class="input-heading-4">Count: <span id="count"></span></div>
          <div id="form-list">
          </div>
          <div class="btn-div">
            <button class="input-button bt-left">Cancel</button>
            <button class="input-button bt-right" type="submit">Submit</button>

          </div>
          
        </form>
      </div>

    </div>



  <script src="/js/addteacher.js"></script>
</body>

</html>

