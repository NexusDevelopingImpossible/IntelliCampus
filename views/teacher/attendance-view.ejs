<div class="tab-header">
    <select id="att-choose" class="att-chose minimal" name=""onclick="view_update(this.value)">
        <option value="view" onclick="view_update(this.value)">View</option>
        <option value="add" onclick="view_update(this.value)">Add</option>
        <option value="update" onclick="view_update(this.value)">Update</option>
    </select>
    

    <div class=" search-input">
        <input type="text" placeholder="Search" />
        <div class="search-btn">
            <a href="#">Search</a>
        </div>
    </div>
    <div class="export-btn">
        <a href="#">Export to excel</a>
    </div>
</div>

<main class="student-profile">
    <div id="list">
        <div class="serial_naming">
            <div class="Alphabet">
                <h1 class="order"></h1>
            </div>
            <div class="stu_list">
                <ul class="stu_name_cards">
                    
                    <%for(let i=0; i<student.length; i++){%>
                        <a href="/teacher/viewstudentattendance?id=<%= student[i]._id %>">
                                <li>
                                    <div class="postcard">
                                        <div class="mark <%if(parseInt(((student[i].totalpresent) /(student[i].present.length))*100)>=  74){%>
                                    present <%}else{%>absent<%}%>
                                    "></div>
                                        <div><img
                                                src="/images/290120_account_avatar_man_profile_user_icon 1.png"
                                                alt="" class="profile_photo"></div>
                                        <div>
                                            <p class="stu_name">
                                                <%= student[i].studentid.name%>
                                            </p>
                                        </div>
                                        <div>
                                            <p class="stu_reg_no">
                                                <%= student[i].studentid.username%>
                                            </p>
                                        </div>
                                    </div>
                                </li>
                            </a>
                        
                        <%}%>
                </ul>
            </div>
        </div>
    </div>
</main>
